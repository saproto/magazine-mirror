	}, false);

	// capture all clicks and store x, y coords for later
	document.addEventListener('click', function(event) {
		if(event.target === cover && !event.synthetic) {
				pos.x = event.clientX;
				pos.y = event.clientY;
				clicked = true;
		}
	}, false);
}
